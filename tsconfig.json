{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"module": "commonjs",
		"outDir": "dist",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es5",
		"lib": ["ES2019.Array"],
		"paths": {
			"@webantic/nginx-config-parser": [
				"./src/models/nginx-config-parser"
			]
		}
	}
}
